}\n\n/* WHITE SECTION */\n.white-box {\n    margin-top: -0.25rem;\n    height: 6rem;\n    background-color: var(--event-text);\n    display: flex;\n    width: 100%;\n    flex-direction: row;\n    margin-bottom: 3rem;\n}\n.message {\n    display: flex;\n    align-items: center;\n}\n.message h2 {\n    text-align: center;\n    padding-right: 1rem;\n}\n\n/* TALKERS & FORM SECTION */\n.talkers-cta {\n    display: flex;\n    width: 100%;\n    flex-direction: row;\n    flex-wrap: wrap;\n}\n.first-marker {\n    background-color: #bedaa1;\n    transform: skew(10deg, 1deg);\n    align-items: center;\n    padding: 1rem;\n}\n.marker-text {\n    text-align: center;\n    transform: skew(-10deg, -1deg);\n}\n.talkers {\n    flex-direction: row;\n    flex-wrap: wrap;\n    column-count: 1;\n    justify-content: center;\n    width: 100%;\n    padding: 4rem;\n}\n.talkers img {\n    width: 10rem;\n    margin: auto;\n}\n.talker {\n    text-align: center;\n    padding-bottom: 2rem;\n}\n.second-marker {\n    background-color: #bedaa1;\n    transform: skew(10deg, 1deg);\n    padding: 1rem;\n}\n.container {\n    display: flex;\n    max-width: 300px;\n    padding: 3rem;\n    border-radius: 2rem;\n}\n.form-group p {\n    padding-bottom: 0rem;\n}\n\n.form-control {\n    border-radius: 0.3rem;\n    border: solid 1px #e4d9c9;\n    margin: 1rem 0 1rem 0;\n    width: 200px;\n    height: 40px;\n    font-size: 18px;\n    color: var(--main-color);\n    padding: 0.5rem;\n}\n.register-button {\n    padding-top: 2rem;\n}\n\n.event-button {\n    display: block;\n    background: transparent;\n    cursor: pointer;\n    position: relative;\n    width: 86px;\n    box-sizing: border-box;\n    width: 100%;\n    height: 58px;\n    border: 0;\n    color: white;\n}\n\n.event-button__text {\n    position: absolute;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    top: 0;\n    right: 8px;\n    bottom: 8px;\n    left: 0;\n    box-sizing: border-box;\n    font-size: 16px;\n    border-radius: 1rem;\n    background-color: var(--main-color);\n}\n\n.event-button::before {\n    content: \"\";\n    position: absolute;\n    border: 2px solid var(--main-color);\n    box-shadow: 0 0 8px 0 var(--main-color) (0, 0, 0, 0.25);\n    top: 4px;\n    right: 0;\n    bottom: 0;\n    left: 4px;\n    border-radius: 1.2rem;\n}\n.event-button:active {\n    outline: none;\n}\n.event-button:active .event-button__text {\n    transform: translateX(6px) translateY(6px);\n    transition: 0.1s;\n}\n\n.event-button:active .event-button__text {\n    box-shadow: none;\n    border-radius: 1rem;\n}\n\n/* INFO & PRODUCT */\n.info-product {\n    padding: 1rem;\n    width: 100%;\n    padding-bottom: 3rem;\n}\n.event-info {\n    padding-bottom: 3rem;\n}\n/* PRODUCT SECTION */\n.products {\n    width: 100%;\n    padding: 1rem 0 1rem 0;\n}\n.product-button {\n    background-color: var(--main-color);\n    box-shadow: 1px 1px 5px black;\n    width: 11rem;\n    height: 11rem;\n    border-radius: 50%;\n    position: absolute;\n    right: 0.4rem;\n    margin-top: -2rem;\n    font-family: \"CaveatBrush\";\n    color: white;\n    font-size: 18px;\n    border: none;\n}\n.hover-text {\n    display: none;\n}\n\n.scroll-up-wrapper {\n    display: flex;\n}\n.scroll-up {\n    cursor: pointer;\n    border: none;\n    background-color: transparent;\n    font-size: 18px;\n    padding-bottom: 2rem;\n}\n.scroll-up:active {\n    outline: 0;\n}\n"],"names":[],"version":3,"file":"index.HASH_REF_14750a40c88cd52c0e40067823a3846a.css.map"}